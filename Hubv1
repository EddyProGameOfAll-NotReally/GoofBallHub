local BLADEAAAA = Blade.SlashPart

coroutine.resume(coroutine.create(function()
	while not stopeverything do
		for i = 0,1,(1/60) do
			swait()
			if not alreadyfixing then
				if mde == "LC" then
					GUN.NeonParts.Color = Color3.new(1,1,1)
