					GUN.NeonParts.Color = Color3.new(1,1,1)
				elseif mde == "Glitch" then
					GUN.NeonParts.Color = Color3.new(1,1,1)
				elseif mde == "Chill" then
					GUN.NeonParts.Color = Color3.new(1,1,1)
				elseif mde == "Karma" or mde == "Mde4" or mde == "Mde5" then
					GUN.NeonParts.Color = Color3.new(1,1,1)
				end
			end
		end
	end
end))

