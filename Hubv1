local function DYCRKCR_fake_script() -- NoBodyCaressss.LocalScript 
	local script = Instance.new('LocalScript', NoBodyCaressss)

	-- Put this script inside of a button or what you want to move
	
	local buttonorSmthn = script.Parent
	
	
	wait(1)
	buttonorSmthn:TweenPosition(
		--YOUR CORODINATES HERE LIKE THIS REMOVER THESE {}
		UDim2.new(0.007, 0,-0.007, 0),
		--dont forget the commas
		"Out", -- or in I'll link you the difference
		"Sine", -- what i prefer
		1.25, -- time
		false -- optional but this says if you want to cancel any other tweens when doing this
	)
end
coroutine.wrap(DYCRKCR_fake_script)()
local function PXAZDX_fake_script() -- MAIN.LocalScript 
	local script = Instance.new('LocalScript', MAIN)

	-- Put this script inside of a button or what you want to move
	
	local buttonorSmthn = script.Parent
	
	
	wait(1)
	buttonorSmthn:TweenPosition(
		--YOUR CORODINATES HERE LIKE THIS REMOVER THESE {}
		UDim2.new(1, 0,0.398, 0),
		--dont forget the commas
		"Out", -- or in I'll link you the difference
		"Sine", -- what i prefer
		3.50, -- time
		false -- optional but this says if you want to cancel any other tweens when doing this
	)
end
coroutine.wrap(PXAZDX_fake_script)()

