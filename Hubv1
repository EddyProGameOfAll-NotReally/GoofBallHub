Head2.Parent = Character
local HeadHandle = CreateWeldOrSnapOrMotor("Weld", Head2.Handle, Head, Head2.Handle, CF(0, 0, 0)*ANGLES(RAD(0),RAD(-90),RAD(0)), CF(0, 0, 0))

